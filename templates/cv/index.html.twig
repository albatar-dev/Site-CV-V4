{% extends 'base.html.twig' %}

{% block body %}

	<div class="container">
		<section class="row">
			{% for categorie in categories %}
				<article class="card w-100 my-3">
					<h5 class="card-header">
						<strong>{{ categorie.catCv }}</strong>
					</h5>
					<div class="card-body">
						<p class="card-text">{{categorie.catCv}}</p>
					</div>
				</article>

			{% endfor %}

		</section>
	</div>
{% endblock %}
